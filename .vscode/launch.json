{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "fsm_gpu_base",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "${workspaceFolder}/GraphMiner/bin/fsm_gpu_base",
            "args": "inputs/mico/graph 2 300",
            "cwd": "${workspaceFolder}/GraphMiner",
            "preLaunchTask": "build_nvcc",
            "initCommands": ["-enable-pretty-printing"]
        },
        {
            "name": "CUDA C++: Attach",
            "type": "cuda-gdb",
            "request": "attach",
            "processId": "${command:cuda.pickProcess}"
        },
        {
            "name": "wxsightgl",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/AutoCellLibX/tools/astran/wxSightGL/build/bin/wxsightgl",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}